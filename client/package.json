{
  "name": "job-tracker-client",
  "version": "1.0.0",
  "description": "client for job tracker",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "lint": "eslint -c ./lib/eslint/eslintrc.json ./src/ts/*",
    "lint:fix": "eslint -c ./lib/eslint/eslintrc.json ./src/ts/* --fix",
    "lint:quiet": "eslint -c ./lib/eslint/eslintrc.json ./src/ts/* --quiet",
    "start": "npm run clean && webpack-dev-server --mode development --config ./lib/webpack/webpack.config.js",
    "build": "npm run clean && webpack --mode production --config ./lib/webpack/prod.webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "DefiantBidet <James Dean>",
  "license": "ISC",
  "proxy": "http://localhost:3030",
  "dependencies": {
    "grommet": "2.31.0",
    "grommet-icons": "4.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.10.0",
    "styled-components": "5.3.9"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.4",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@types/webpack": "5.28.1",
    "babel-loader": "9.1.2",
    "babel-plugin-module-resolver": "5.0.0",
    "dotenv": "16.0.3",
    "eslint": "8.38.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-loader": "4.2.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "29.5.0",
    "msw": "1.2.1",
    "prettier": "2.8.7",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "typescript": "5.0.4",
    "webpack": "5.79.0",
