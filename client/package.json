{
  "name": "job-tracker-client",
  "version": "1.0.0",
  "description": "client for job tracker",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "lint": "eslint ./src/ts/*",
    "lint:fix": "eslint ./src/ts/* --fix",
    "lint:quiet": "eslint ./src/ts/* --quiet",
    "start": "npm run clean && npm run build:css && webpack-dev-server --mode development --config ./src/lib/webpack/webpack.config.js",
    "build": "npm run clean && npm run build:css && webpack --mode production --config ./src/lib/webpack/prod.webpack.config.js",
    "build:css": "node-sass --include-path node_modules/ src/scss/app.scss -o dist",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "DefiantBidet <James Dean>",
  "license": "ISC",
  "proxy": "http://localhost:3030",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@types/webpack": "^5.28.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "css-loader": "^6.7.3",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "msw": "^1.2.1",
    "node-sass": "^8.0.0",
    "postcss-loader": "^7.2.4",
    "postcss-preset-env": "^8.3.0",
    "prettier": "^2.8.7",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.3",
    "uuid": "^9.0.0",
    "webpack": "^5.77.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  }
}
